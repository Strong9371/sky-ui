.pageview{
  width: 100vw;
  height: 100%;
  display: flex;
}
.popRouteview{
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.maskview{
  width: 100vw;height: 100%;position: absolute;top: 0px;
}
.container{
  position: absolute;
  overflow: hidden;
}

.upRoute{
  bottom: 0px;
}
.downRoute{
  top:0px
}
.leftRoute{
  right: 0px;
}
.rightRoute{
  left:0px
}
